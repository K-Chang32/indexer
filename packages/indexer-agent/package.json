{
  "name": "@graphprotocol/indexer-agent",
  "version": "0.0.0",
  "description": "Indexer agent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/graphprotocol/indexer",
  "author": "Graph Protocol",
  "private": false,
  "scripts": {
    "compile": "tsc",
    "format": "prettier --write 'src/*.ts'",
    "prepare": "yarn compile",
    "start": "yarn compile && node ./dist/index.js start --graph-node-admin-endpoint http://localhost:8020/ --graph-node-status-endpoint http://localhost:8030/graphql"
  },
  "dependencies": {
    "@graphprotocol/common-ts": "0.0.21",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.15",
    "global": "^4.4.0",
    "graphql": "^14.0.0",
    "graphql-tag": "^2.10.1",
    "jayson": "^3.3.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/yargs": "^15.0.4"
  }
}
